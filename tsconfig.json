{
  "compilerOptions": {
    // "target": "esnext",
    "module": "esnext",
    "target": "es5",
    // "module": "es2020",
    "strict": true,
    "jsx": "preserve",
    // "importHelpers": true,
    "moduleResolution": "node",
    // "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    // "noImplicitAny": true,
    // "noImplicitReturns": true,
    // "noImplicitThis": true,
    "baseUrl": ".",
    // "types": [
    //   "webpack-env"
    // ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
  ],
  "exclude": [
    "node_modules",
    "src/**/*.js"
  ]
}